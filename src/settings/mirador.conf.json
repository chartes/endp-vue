{
  "themes": {
    "dark": {
      "overlayFont": "Chivo Mono",
      "palette": {
        "primary": {
          "main": "#b00722"
        },
        "secondary": {
          "main": "#52b3fc"
        }
      }
    }
  },
  "window": {
    "sideBarOpenByDefault": false,
    "allowClose": false,
    "allowFullscreen": true,
    "allowMaximize": false,
    "hideWindowTitle": true,
    "defaultView": "single",
    "views": [
      {
        "key": "single",
        "behaviors": [
          "individuals"
        ]
      }
    ],
    "textOverlay": {
      "enabled": true,
      "selectable": false,
      "visible": false,
      "useAutoColors": true
    }
  },
  "workspaceControlPanel": {
    "enabled": false
  },
  "workspace": {
    "showZoomControls": true,
    "type": "mosaic"
  }
}